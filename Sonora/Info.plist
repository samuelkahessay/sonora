<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.samuelkahessay.Sonora</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>sonora</string>
			</array>
		</dict>
	</array>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>NSCalendarsFullAccessUsageDescription</key>
	<string>Sonora needs full calendar access to check for conflicts and suggest the best calendar for your events.</string>
	<key>NSCalendarsUsageDescription</key>
	<string>Sonora can add detected events from your voice memos directly to your calendar for better organization.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Sonora needs microphone access to record voice memos. Your recordings stay private on your device until you choose to transcribe them using AI.</string>
	<key>NSRemindersUsageDescription</key>
	<string>Sonora can create reminders from your voice memos to help you stay on top of important tasks.</string>
	<key>NSSupportsLiveActivities</key>
	<true/>
	<key>NSSupportsLiveActivitiesFrequentUpdates</key>
	<false/>
	<key>UIAppFonts</key>
	<array>
		<string>NewYorkMedium-Regular.otf</string>
		<string>NewYorkMedium-Medium.otf</string>
		<string>NewYorkMedium-Semibold.otf</string>
		<string>NewYorkMedium-Bold.otf</string>
		<string>NewYorkMedium-Heavy.otf</string>
		<string>NewYorkMedium-Black.otf</string>
		<string>NewYorkMedium-RegularItalic.otf</string>
		<string>NewYorkMedium-MediumItalic.otf</string>
		<string>NewYorkMedium-SemiboldItalic.otf</string>
		<string>NewYorkMedium-BoldItalic.otf</string>
		<string>NewYorkMedium-HeavyItalic.otf</string>
		<string>NewYorkMedium-BlackItalic.otf</string>
	</array>
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
	</array>
</dict>
</plist>
