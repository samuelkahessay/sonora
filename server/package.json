{
  "name": "sonora-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx src/server.ts",
    "dev:pretty": "NODE_ENV=development LOG_LEVEL=debug tsx src/server.ts",
    "dev:json": "NODE_ENV=development LOG_LEVEL=info tsx src/server.ts | cat",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "npm run build --silent && node --test tests/*.test.mjs",
    "start:prod": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@sentry/node": "^10.24.0",
    "@sentry/profiling-node": "^10.24.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "undici": "^6.21.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
